    "cpu": "256",
    "executionRoleArn": "non-prod-capstone-ecsTaskExecutionRole",
    "family": "frontend",
    "taskRoleArn": "non-prod-capstone-ecsTaskExecutionRole",
    "memory": "256",
    "networkMode": "bridge"
}

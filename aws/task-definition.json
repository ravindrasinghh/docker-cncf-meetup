{
    "containerDefinitions": [{
        "essential": true,
        "image": "240633844458.dkr.ecr.ap-south-1.amazonaws.com/web:latest",
        "name": "frontend",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
        }]

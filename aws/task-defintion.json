{
    "containerDefinitions": [{
        "essential": true,
        "image": "240633844458.dkr.ecr.us-west-2.amazonaws.com/nginx:latest",
        "name": "nginx-container",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 0,
            "protocol": "tcp"
        }]
    }],
    "cpu": "256",
    "executionRoleArn": "ecsTaskExecutionRole",
    "family": "nginx-container",
    "taskRoleArn": "ecsTaskExecutionRole",
    "memory": "256",
    "networkMode": "bridge"
}